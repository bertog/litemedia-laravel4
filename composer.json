{
	"name": "Litemedia/laravel4",
	"description": "Laravel Blueprint for Litemedia Laravel App.",
	"keywords": ["framework", "laravel", "litemedia"],
	"license": "MIT",
	"type": "project",
	"require": {
		"laravel/framework": "4.2.*",
		"laracasts/commander": "1.3.10",
		"laracasts/flash": "1.2",
		"laracasts/validation": "1.2.2",
        "intervention/image": "~2.0"
	},
    "require-dev": {
        "way/generators": "2.6.1",
        "codeception/codeception": "~2.0",
        "laracasts/testdummy": "~2.0",
        "phpspec/phpspec": "~2.1",
        "barryvdh/laravel-ide-helper": "~1.11"
    },
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		],
        "psr-4": {
            "ChangeMe\\": "app/ChangeMe"
        }
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "stable"
}
